{
  "name": "shubupad-backend",
  "version": "1.0.0",
  "description": "ShubUpad Recipe Platform - Backend API",
  "main": "dist/server.js",
  "scripts": {
    "dev": "nodemon src/server.ts",
    "build": "tsc",
    "start": "node dist/server.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:e2e": "jest --config jest.e2e.config.js",
    "migrate": "ts-node src/database/migrations/run-migrations.ts",
    "seed": "ts-node src/database/seeds/run-seeds.ts",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "format": "prettier --write src/**/*.ts",
    "docker:build": "docker build -t shubupad-backend .",
    "docker:run": "docker-compose up",
    "clean": "rm -rf dist",
    "postinstall": "echo 'Installation complete!'"
  },
  "keywords": [
    "recipe",
    "cooking",
    "nodejs",
    "typescript",
    "api",
    "food"
  ],
  "author": "ShubUpad Team",
  "license": "MIT",
  "dependencies": {
    "@elastic/elasticsearch": "^8.15.0",
    "@sendgrid/mail": "^8.1.3",
    "aws-sdk": "^2.1691.0",
    "bcryptjs": "^2.4.3",
    "bull": "^4.16.3",
    "compression": "^1.7.4",
    "connect-redis": "^7.1.1",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "csrf": "^3.1.0",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "express-rate-limit": "^7.4.1",
    "express-session": "^1.18.1",
    "express-slow-down": "^2.0.3",
    "express-validator": "^7.2.0",
    "helmet": "^7.1.0",
    "hpp": "^0.2.3",
    "ioredis": "^5.4.1",
    "joi": "^17.13.3",
    "jsonwebtoken": "^9.0.2",
    "lodash": "^4.17.21",
    "moment": "^2.30.1",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "node-cron": "^3.0.3",
    "nodemailer": "^6.9.15",
    "pg": "^8.12.0",
    "redis": "^4.7.0",
    "sharp": "^0.33.5",
    "stripe": "^16.12.0",
